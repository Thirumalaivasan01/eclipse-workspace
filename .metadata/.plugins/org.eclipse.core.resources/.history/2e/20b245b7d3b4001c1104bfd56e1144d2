package com.te.spring.core.assignment2.dto;

public class Drinks {
	private String milkShake;
	private String cocaCola;
	private String appleJuice;

	public String getMilkShake() {
		milkShake = "MilkShake";
		return milkShake;
	}

	public String getCocaCola() {
		cocaCola = "CocaCola";
		return cocaCola;
	}

	public String getAppleJuice() {
		appleJuice = "AppleJuice";
		return appleJuice;
	}

	@Override
	public String toString() {
		return "Drinks [milkShake=" + milkShake + ", cocaCola=" + cocaCola + ", appleJuice=" + appleJuice + "]";
	}

}
