package com.te.spring.core.assignment2.dto;

public class Food {
	private String biriyani ;
	private String idli ;
	private String friedRice ;
	public Food() {
		super();
		// TODO Auto-generated constructor stub
	}
	public String getBiriyani() {
		return biriyani;
	}
	public void setBiriyani(String biriyani) {
		this.biriyani = biriyani;
	}
	public String getIdli() {
		return idli;
	}
	public void setIdli(String idli) {
		this.idli = idli;
	}
	public String getFriedRice() {
		return friedRice;
	}
	public void setFriedRice(String friedRice) {
		this.friedRice = friedRice;
	}
	@Override
	public String toString() {
		return "Food [biriyani=" + biriyani + ", idli=" + idli + ", friedRice=" + friedRice + "]";
	}
	

}
