package exam;

import java.util.*;

public class ex2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int m = 3, n = 3;
		int[][] mat = new int[m][n];
		int[][] newmat = new int[m + 2][n + 2];
		ArrayList<Integer> arr = new ArrayList<Integer>();
		for (int i = 0; i < m; i++) {
			for (int j = 0; j < n; j++) {
				mat[i][j] = sc.nextInt();

			}
			System.out.println();
		}

		for (int i = 0; i < m + 1; i++) {
			for (int j = 0; j < n + 1; j++) {
				// First row and last row
				if (i == 0 || i == m + 1)
					newmat[i][j] = 0;
				else {
					// Fist column and last column
					if (j == 0 || j == n + 1)
						newmat[i][j] = 0;
					else {
						// Content
						newmat[i][j] = mat[i - 1][j - 1];
					}
				}
			}
		}
		for (int i = 0; i < m + 2; i++) {
			for (int j = 0; j < n + 2; j++) {
				System.out.print(newmat[i][j]);
			}
			System.out.println();
		}
		int k=0;
		for (int i = 1; i < m; i++) {
			for (int j = 1; j < n; j++) {
				if (i == j) {
					mat[k++][k++] = newmat[i - 1][j - 1] + newmat[i - 1][j] + newmat[i - 1][j + 1] + newmat[i][j - 1]
							+ newmat[i][j + 1] + newmat[i + 1][j - 1] + newmat[i + 1][j] + newmat[i + 1][j + 1];
				}
			}
		}
		for (int i = 0; i < m ; i++) {
			for (int j = 0; j < n ; j++) {
				System.out.print(mat[i][j]);
			}
			System.out.println();
		}
		
	}
}