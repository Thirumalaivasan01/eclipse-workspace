package exam;

import java.util.Scanner;

public class LogestNonConsecutiveDigits {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String binaryInput = sc.nextLine();
		String subString;
		String[] resultArray = new String[30];
		String maxResult;
		boolean check = false;
		int length = binaryInput.length(), a = 0;
		for (int i = 0; i < length; i++) {
			for (int j = i; j < length; j++) {
				subString = binaryInput.substring(i, j + 1);
				for (int k = 0; k < subString.length(); k++) {
					if (subString.charAt(k) != subString.charAt(k + 1)) {
						check = true;
						break;
					}
				}
				if (check == false) {
					resultArray[a++] = subString;
				} else {
					check = false;
				}
				System.out.println(subString);
			}
		}
		maxResult = resultArray[0];
		for (int i = 0; i < resultArray.length; i++) {
			if (resultArray[i].length() > maxResult.length()) {
				maxResult = resultArray[i];

			}
		}
		sc.close();
	}

}
