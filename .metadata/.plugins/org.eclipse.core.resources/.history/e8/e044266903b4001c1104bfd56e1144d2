package com.te.spring.core.one.beans;

public class Lion implements Creature {
	String name="Lion";
	String type="Human";
	@Override
	public String creatureName() {
		System.out.println("Lion");
		return name;
	}

	@Override
	public String creatureType() {
		System.out.println("Animal");
		return type;
	}

}
