package com.te.learn.oops.abstraction;

public abstract class A {
	public int a, b;

	public A(int a, int b) {
		this();
		this.a = a;
		this.b = b;
	}

	static {
		System.out.println("static initializer of A class is executed");
	}
	{
		System.out.println("non static initializer of A class is executed");
	}

	public A() {
		super();
		// TODO Auto-generated constructor stub
	}

}
